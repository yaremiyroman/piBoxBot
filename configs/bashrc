alias piMedia='ssh pi@piMedia.local'
alias piPhotos='scp -r pi@pi.local:/home/pi/cam/ /Users/rom/Documents/'

alias cc='clear'
alias offf='sudo shutdown -h now'

alias g='git'
alias s='git status'
alias d='git diff'
alias ci='git commit'
alias fetch='git fetch --all'
alias m='git pull origin master'
alias master='git push origin master'
alias d='git pull origin dev'
alias dev='git push origin dev'

alias srv_s='sudo systemctl status bot.service climate.service sensors.service lights.service'
alias srv_en='sudo systemctl enable bot.service climate.service sensors.service lights.service'
alias srv_start='sudo systemctl start bot.service climate.service sensors.service lights.service'
alias srv_stop='sudo systemctl stop bot.service climate.service sensors.service lights.service'
alias srv_dis='sudo systemctl disable bot.service climate.service sensors.service lights.service'
